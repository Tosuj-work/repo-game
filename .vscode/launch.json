{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "kiss-flutter",
            "request": "launch",
            "type": "dart",
            "args": ["-t", "lib/main.dart"]
        },
        {
            "name": "kiss-flutter-web",
            "request": "launch",
            "type": "dart",
            "args": ["-t", "lib/main_web.dart"]
        },
        {
            "name": "kiss-flutter (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": ["-t", "lib/main.dart"]
        },
        {
            "name": "kiss-flutter (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": ["-t", "lib/main_release.dart"]
        },
        {
            "name": "kiss-flutter-web",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": ["-t", "lib/main_web.dart"]
        },
        {
            "name": "kiss-flutter-web (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": ["-t", "lib/main_web.dart"]
        },
        {
            "name": "kiss-flutter-web (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": ["-t", "lib/main_web_release.dart"]
        }
    ]
}
